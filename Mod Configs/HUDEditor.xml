<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<HUDEditor>

	<Mod_Options>
	
		<!--EditMode allows you to view changes to this file in real time-->
		<EditMode>true</EditMode>
		
		<!-- 
			No Clue Why the Default is 1.5. 
			Change at your Own Risk.
		-->
		<MaxScale>1.5</MaxScale>
		<!-- 
			Message Verifying the Mod is Working
			on Hud Load.
			Leave the Sound Section Empty if You Want to Disable Sound
		-->
		<StartupMsg>
			<Enable>false</Enable>
			<Delay>60000</Delay>
			<Sound>UIPerkIdiotSavantLAYERfemale</Sound>
		</StartupMsg>
		
		<!--Settings for HUD Editor WaterMark in the Upper Right-->
		<WaterMark>
			<!--Settings for Default HUD Editor WaterMark-->
			<HideDefault>true</HideDefault>
			<CustomWaterMark>true</CustomWaterMark>
				<CustomWaterMarkEntry>Like And Subscribe</CustomWaterMarkEntry>
			<!--Settings for HUD Editor Edit Mode WaterMark-->
			<HideEditMode>true</HideEditMode>
			<CustomEditMark>true</CustomEditMark>
				<CustomEditMarkEntry>Edit Mode Enabled</CustomEditMarkEntry>
		</WaterMark>
		
	</Mod_Options>
	
	<Reposition_Elements>
		
		<!--
			All X and Y positions are relative to their default position on the HUD.
			Scale is the Size of the Obj
			X is Left and Right
			Y is Up and Down.
			PowerArmor is when you use the Power Armor Hud [Toggleable in the Game Settings]
		-->
		
		<!--Bar Showing Your Crit Charge in Vats-->
			<CritMeter>
				<Scale>1.25</Scale>
				<X>0</X>
				<Y>0</Y>
			</CritMeter>
		
		<!-- Direction and Map Marker Indicator -->
			<Compass>
				<Scale>1.25</Scale>
				<X>0</X>
				<Y>0</Y>
			</Compass>
		
		<!--Hp Bar For Targeted Enemy-->
			<EnemyHealthMeter>
				<Scale>2</Scale>
				<X>0</X>
				<Y>0</Y>
			</EnemyHealthMeter>
		
		<!--Top Right List of Active Quests Tagged in Pipboy Questlog-->
			<QuestTracker>
				<Scale>.70</Scale>
				<X>30</X>
				<Y>-40</Y>
			</QuestTracker>
		
		<!--Sneaking Meter (Hidden, Danger, etc)-->
			<StealthMeter>
				<Scale>1.25</Scale>
				<X>0</X>
				<Y>0</Y>
			</StealthMeter>
		
		<!-- List of Players on Your Team-->
			<TeamPanel>
				<Scale>.80</Scale>
				<X>-60</X>
				<Y>150</Y>
				<PowerArmor>
					<Scale>.80</Scale>
					<X>-60</X>
					<Y>150</Y>
				</PowerArmor>
			</TeamPanel>
		
        <!-- Pop-Up when You Shoot Something -->
			<HitMarker>
				<Scale>1.25</Scale>
				<X>0</X>
				<Y>0</Y>
			</HitMarker>
		
		<InteractPrompts>
			
		<!--Quickloot, annoyingly, applies to some containers rollover-->
			<QuickLoot>
				<Scale>.80</Scale>
				<X>0</X>
				<Y>0</Y>
			</QuickLoot>
			
		<!--Frobber are interact prompts like unlicking locked doors and interacting with NPCs-->
			<Frobber>
				<Scale>.80</Scale>
				<X>0</X>
				<Y>0</Y>
			</Frobber>
			
		<!--RollOver is essentially the same as Frobber (interact prompts)-->
			<RollOver>
				<Scale>.80</Scale>
				<X>0</X>
				<Y>0</Y>
			</RollOver>
			
		</InteractPrompts>
		
		<HUD_Notifications>
		
		<!-- quest/event complete/rewards list -->
			<Announce>
				<Scale>.80</Scale>
				<X>0</X>
				<Y>0</Y>
			</Announce>
			
		<!--Area/Team Voice Chat Indicator that List Players Talking Around You-->		
			<AreaVoiceList>
				<Scale>.80</Scale>
				<X>0</X>
				<Y>0</Y>
			</AreaVoiceList>
			
		<!-- Popup When You Gain / Lose Gold, Caps, Scrip, Ect. -->
			<CurrencyUpdates>
				<Scale>1</Scale>
				<X>0</X>
				<Y>0</Y>
			</CurrencyUpdates>
			
		<!-- Small Emote Circle on the Lower Left Side of your Screen-->	
			<Emote>
				<Scale>.80</Scale>
				<X>0</X>
				<Y>0</Y>
			</Emote>
			
		<!--
			Pop-up When Your Current Fusion Core Is Low or Empty,
			And You Have No Spares
		-->
			<FusionCoreWarning>
				<Scale>2</Scale>
				<X>0</X>
				<Y>0</Y>
			</FusionCoreWarning>
		
		<!-- Pop-up Showing Your New Level -->		
			<LevelUpAnimation>
				<Scale>1</Scale>
				<X>0</X>
				<Y>0</Y>
			</LevelUpAnimation>
			
		<!--Notifications are Pop-Ups Like Sneak damage, New Radio Signels, And World Event Msg Boxes-->
			<Notification>
				<Scale>.80</Scale>
				<X>200</X>
				<Y>-120</Y>
			</Notification>
		
		<!-- 
				Pop-up When Perk Activates Such as
				Ricochet, Nerd Rage, Ect. 
				Dosnt Effect the Sound 
		-->
			<PerkPopUp>
				<Scale>.80</Scale>
				<X>0</X>
				<Y>0</Y>
			</PerkPopUp>
			
		<!-- Pop-Up When Taking Rad Dmg-->
			<RadDamageIndicator>
				<Scale>.80</Scale>
				<X>0</X>
				<Y>0</Y>
			</RadDamageIndicator>
			
		<!-- Change in Faction Rep Pop-Up -->
			<ReputationUpdates>
				<Scale>1</Scale>
				<X>0</X>
				<Y>0</Y>
			</ReputationUpdates>
		
		<!-- Description of Onscreen Voicelines -->		
			<Subtitles>
				<Scale>1</Scale>
				<X>0</X>
				<Y>0</Y>
			</Subtitles>
		
		<!-- Pop-Up Showing XP Gained from Action -->	
			<XPBar>
				<Scale>1</Scale>
				<X>0</X>
				<Y>0</Y>
			</XPBar>
			
		</HUD_Notifications>
		
		<LeftMeters>
			
		<!-- 
			Bar Shown in PA When PA HUD is Set to False
			Tracks Fusion Core % and Amount.
			Auto Detects if in Proper Hud / Class.
		-->		
			<FusionCoreMeter>
				<Scale>1.25</Scale>
				<X>0</X>
				<Y>0</Y>
			</FusionCoreMeter>
			
		<!-- Bar Showing Your Current Health -->	
			<HpMeter>
				<Scale>1</Scale>
				<X>0</X>
				<Y>0</Y>
			</HpMeter>
			
		<!--
			Bar for Ghouls that Shows Current Glow Level
			Only Applies if Your Ghoul Class, Auto Detects
		-->
			<GlowMeter>
				<Scale>1</Scale>
				<X>2.5</X>
				<Y>0</Y>
			</GlowMeter>
			
		</LeftMeters>
		
		<RightMeters>
			
