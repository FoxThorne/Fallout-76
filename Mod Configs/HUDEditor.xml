			<ActiveEffects>
				<Scale>.80</Scale>
				<X>0</X>
				<Y>0</Y>
				<PowerArmor>
					<X>0</X>
					<Y>0</Y>
				</PowerArmor>
			</ActiveEffects>
			
		<!-- Shows Current Amount of Ammo in Selected Gun -->	
