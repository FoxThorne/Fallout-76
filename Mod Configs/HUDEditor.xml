			<OnlyNegativeEffects>true</OnlyNegativeEffects>
		</ActiveEffects>
		
		<Ap>
			<Hide>false</Hide>
			<Crouch>false</Crouch>
			<VaTs>false</VaTs>
			<Fade>false</Fade>
				<FadePer>25</FadePer>
