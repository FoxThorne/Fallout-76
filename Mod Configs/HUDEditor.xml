				<Scale>.80</Scale>
				<X>-20</X>
				<Y>-10</Y>
			</AmmoCount>
		
		<!-- Bar Showing Current Ap Total -->
			<APMeter>
				<Scale>1</Scale>
				<X>0</X>
				<Y>0</Y>
			</APMeter>
			
		<!-- Grenade / Throwable Counter -->
			<ExplosiveAmmoCount>
				<Scale>.80</Scale>
				<X>-40</X>
				<Y>10</Y>
				<PowerArmor>
					<X>0</X>
					<Y>0</Y>
				</PowerArmor>
			</ExplosiveAmmoCount>
			
		<!--
			Bar for Ghouls that Shows Current Feral Level
			Only Applies if Your Ghoul Class, Auto Detects
		-->
			<FeralMeter>
				<Scale>.80</Scale>
				<X>0</X>
				<Y>10</Y>
			</FeralMeter>
		
		<!--
			Bar used by the V63 Carbine Weapon
